{
  "name": "@lerna-monorepo/api",
  "description": "Example API using lerna",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers",
    "api"
  ],
  "author": {
    "name": "Rodolfo Olivieri",
    "email": "rodolfo.olivieri@nicht.rocks"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run compile && yarn run mocha",
    "dev": "ts-node-dev --no-notify src/",
    "start": "yarn run compile && node lib/",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.1",
    "@feathersjs/authentication-local": "^4.5.1",
    "@feathersjs/authentication-oauth": "^4.5.1",
    "@feathersjs/configuration": "^4.5.1",
    "@feathersjs/errors": "^4.5.1",
    "@feathersjs/express": "^4.5.1",
    "@feathersjs/feathers": "^4.5.1",
    "@feathersjs/primus": "^4.5.1",
    "@feathersjs/transport-commons": "^4.5.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-sequelize": "^6.1.0",
    "helmet": "^3.21.2",
    "pg": "^8.1.0",
    "sequelize": "^5.21.4",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.46",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mocha": "^7.0.2",
    "@types/serve-favicon": "^2.5.0",
    "@types/validator": "^10.0.0",
    "axios": "^0.19.2",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "shx": "^0.3.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
